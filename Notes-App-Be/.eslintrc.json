{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "eslint-comments/no-unused-disable": "off",
        "max-len": ["error", { "comments": 200 }]
    }
}
